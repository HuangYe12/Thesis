db );\u003Cbr \/\u003EcublasSetMatrixAsync( rows, cols, elemSize, A_src_host, lda, B_dst_dev,  ldb, stream );\u003Cbr \/\u003EcublasGetMatrixAsync( rows, cols, elemSize, A_src_dev,  lda, B_dst_host, ldb, stream );\u003Cbr \/\u003E\u003C\/pre\u003E\u003Ch3 style=\"font-family: Optima, Verdana, sans-serif; margin: 0.83em 0px 0.5em;\"\u003EConstants\u003C\/h3\u003E\u003Ctable style=\"border-collapse: collapse;\"\u003E\u003Ctbody\u003E\u003Ctr\u003E\u003Cth style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003Eargument\u003C\/th\u003E\u003Cth style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003Econstants\u003C\/th\u003E\u003Cth style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003Edescription (Fortran letter)\u003C\/th\u003E\u003C\/tr\u003E\u003Ctr\u003E\u003Ctd style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003E\u003Ccode style=\"font-size: 9pt; white-space: pre;\"\u003Etrans\u003C\/code\u003E\u003C\/td\u003E\u003Ctd style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003E\u003Ccode style=\"font-size: 9pt; white-space: pre;\"\u003ECUBLAS_OP_N               \u003C\/code\u003E\u003C\/td\u003E\u003Ctd style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003Enon-transposed ('N')\u003C\/td\u003E\u003C\/tr\u003E\u003Ctr\u003E\u003Ctd style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003E\u003C\/td\u003E\u003Ctd style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003E\u003Ccode style=\"font-size: 9pt; white-space: pre;\"\u003ECUBLAS_OP_T               \u003C\/code\u003E\u003C\/td\u003E\u003Ctd style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003Etransposed ('T')\u003C\/td\u003E\u003C\/tr\u003E\u003Ctr\u003E\u003Ctd style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003E\u003C\/td\u003E\u003Ctd style=\"padding: 1px 1em 1px 0px; vertical-align: top;\"\u003E\u003Ccode style=\"font-size: 9pt; white-space: pre;\"\u003ECUBLAS_OP_C               \u003C\/code\u003E\u003C\/td\u003E\u003Ctd style=\"padding: 1p